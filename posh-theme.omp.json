{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "palette": {
    "_start-flavours": "",
"base00": "#071011",
"base01": "#0f2224",
"base02": "#5e7173",
"base03": "#849496",
"base04": "#aab8ba",
"base05": "#e1edef",
"base06": "#d7e1e3",
"base07": "#dee6e8",
"base08": "#5d7c92",
"base09": "#677d78",
"base0A": "#2b8948",
"base0B": "#d34f33",
"base0C": "#6e7c6b",
"base0D": "#d6bf49",
"base0E": "#2781a1",
"base0F": "#28829f",
    "_end-flavours": ""
  },
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "foreground": "p:base03",
          "properties": {
            "folder_separator_icon": "/",
            "style": "full"
          },
          "style": "plain",
          "template": " \uf07b {{ .Path }} ",
          "type": "path"
        },
        {
          "foreground": "p:white",
          "powerline_symbol": "\ue0b0",
          "properties": {
            "fetch_stash_count": true,
            "fetch_upstream_icon": true
          },
          "style": "plain",
          "template": "{{ .UpstreamIcon }}{{ .HEAD }}{{ if gt .StashCount 0 }} \ueb4b {{ .StashCount }}{{ end }} ",
          "type": "git"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "foreground": "p:base07",
          "style": "plain",
          "template": "\ue3bf ",
          "type": "root"
        },
        {
          "foreground": "p:base07",
          "style": "plain",
          "template": ">  ",
          "type": "text"
        }
      ],
      "type": "prompt"
    }
  ],
  "version": 3
}
